{
    "name": "devanasdeveloper/laravel-categories",
    "description": "A package for category management in Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DevanasDeveloper",
            "email": "devanas.anas@gmail.com"
